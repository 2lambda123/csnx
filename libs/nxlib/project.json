{
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "libs/nxlib/src",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nrwl/js:tsc",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/libs/nxlib",
				"main": "libs/nxlib/src/index.ts",
				"tsConfig": "libs/nxlib/tsconfig.lib.json",
				"assets": ["libs/nxlib/*.md"]
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["libs/nxlib/**/*.ts"]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["coverage/libs/nxlib"],
			"options": {
				"jestConfig": "libs/nxlib/jest.config.ts",
				"passWithNoTests": true
			}
		}
	},
	"tags": []
}
