{
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "libs/nrwl-rollup",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nrwl/web:rollup",
			"outputs": ["{options.outputPath}"],
			"options": {
				"project": "libs/nrwl-rollup/package.json",
				"entryFile": "libs/nrwl-rollup/src/index.ts",
				"outputPath": "dist/libs/nrwl-rollup",
				"tsConfig": "libs/nrwl-rollup/tsconfig.lib.json",
				"packageJson": "libs/nrwl-rollup/package.json",
				"main": "libs/nrwl-rollup/src/index.ts",
				"assets": ["libs/nrwl-rollup/readme.md"]
			},
			"format": ["esm", "cjs"]
		}
	},
	"tags": []
}
