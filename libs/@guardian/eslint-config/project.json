{
	"$schema": "../../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "libs/@guardian/eslint-config",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@csnx/npm-package:pack",
			"outputs": ["{options.outputPath}"],
			"options": {
				"project": "libs/@guardian/eslint-config/package.json",
				"outputPath": "dist/libs/@guardian/eslint-config"
			}
		},
		"e2e": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["coverage/libs/@guardian/eslint-config"],
			"options": {
				"jestConfig": "libs/@guardian/eslint-config/jest.config.js"
			}
		}
	},
	"tags": []
}
